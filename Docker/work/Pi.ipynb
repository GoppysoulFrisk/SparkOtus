   "source": [
    "val partitions = 10\n",
    "val n = 100000 * partitions\n",
    "val count = spark.sparkContext.parallelize(1L to n).map { i =>\n",
    "  val x = math.random * 2 - 1\n",
    "  val y = math.random * 2 - 1\n",
    "  if (x * x + y * y <= 1) 1 else 0\n",
    "}.reduce(_ + _)\n",
    "\n",
    "val pi = (4.0 * count) / n\n",
    "println(s\"Pi is roughly $pi\")"
   ]
  }
